{
  "name": "@mytiki/l0-auth-plugin",
  "version": "0.2.1",
  "description": "Plugin for easy client interfacing with the L0 Auth API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "wrangler dev --local",
    "build": "tsc --project tsconfig.build.json",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "test": "jest --config jest.config.json --passWithNoTests",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "postversion": "npm run format",
    "deploy": "wrangler publish",
    "clean": "tsc --build --clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tiki-bar/l0-auth-plugin"
  },
  "keywords": [
    "TIKI",
    "L0 Auth"
  ],
  "author": {
    "name": "Tiki inc.",
    "email": "hello@mytiki.com",
    "url": "https://mytiki.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tiki-bar/l0-auth-plugin/issues"
  },
  "homepage": "https://github.com/tiki-bar/l0-auth-plugin#readme",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist/**/*"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {}
}
